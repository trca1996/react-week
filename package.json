{
    "name": "my-next-app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "next build",
        "start": "next start",
        "ssl:proxy": "local-ssl-proxy --key ./certs/key.pem --cert ./certs/certificate.pem --source 3001 --target 3000",
        "dev": "next dev",
        "dev:ssl": "concurrently \"yarn dev\" \"yarn ssl:proxy\"",
        "format": "prettier --write \"src/**/*.{ts,tsx}\"",
        "lint": "prettier --check \"src/**/*.{ts,tsx}\"",
        "lint:eslint": "eslint --format unix \"**/*.ts\" \"**/*.tsx\"",
        "lint:fix": "yarn lint:eslint --fix",
        "lint:all": "yarn lint && yarn lint:eslint",
        "lint:style": "stylelint \"**/*.css\"",
        "reset": "rm -rf node_modules && rm -rf .next && yarn",
        "build-storybook": "build-storybook",
        "sb": "start-storybook -p 6006",
        "prepare": "husky install"
    },
    "dependencies": {
        "axios": "^1.2.1",
        "next": "13.0.6",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.20.5",
        "@storybook/addon-actions": "^6.5.14",
        "@storybook/addon-essentials": "^6.5.14",
        "@storybook/addon-interactions": "^6.5.14",
        "@storybook/addon-links": "^6.5.14",
        "@storybook/builder-webpack5": "^6.5.14",
        "@storybook/manager-webpack5": "^6.5.14",
        "@storybook/react": "^6.5.14",
        "@storybook/testing-library": "^0.0.13",
        "@types/node": "18.11.12",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.9",
        "@typescript-eslint/eslint-plugin": "^5.46.0",
        "@typescript-eslint/parser": "^5.46.0",
        "babel-loader": "^9.1.0",
        "concurrently": "^7.6.0",
        "eslint": "8.29.0",
        "eslint-config-next": "13.0.6",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.11",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-storybook": "^0.6.8",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "^8.0.2",
        "local-ssl-proxy": "^1.3.0",
        "prettier": "^2.8.1",
        "stylelint": "^14.16.0",
        "stylelint-config-prettier": "^9.0.4",
        "stylelint-config-standard": "^29.0.0",
        "typescript": "4.9.4"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": "please-use-yarn",
        "yarn": ">=1.22.0"
    }
}
